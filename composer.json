{
    "name": "ahs/ninjs-php",
    "description": "Serialize ninjs objects representation into json. Deserialize ninjs into objects",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "symfony/serializer": "^3.2",
        "symfony/property-access": "^3.2",
        "swp/bridge": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AHS\\Ninjs\\": "src/AHS/Ninjs/"
        },
        "exclude-from-classmap": [ "Tests/*" ]
    },
    "autoload-dev": {
        "psr-4": {
            "AHS\\Ninjs\\Tests\\": "Tests/"
        }
    },
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Paweł Mikołajczuk",
            "email": "mikolajczuk.private@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
